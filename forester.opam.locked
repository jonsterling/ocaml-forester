opam-version: "2.0"
name: "forester"
version: "2.5"
synopsis: "A tool for tending mathematical forests"
maintainer: "Jonathan Sterling"
authors: "Jonathan Sterling"
license: "GPL-3.0-or-later"
homepage: "https://sr.ht/~jonsterling/forester/"
bug-reports: "https://todo.sr.ht/~jonsterling/forester"
depends: [
  "algaeff" {= "1.1.0"}
  "base-bigarray" {= "base"}
  "base-domains" {= "base"}
  "base-nnp" {= "base"}
  "base-threads" {= "base"}
  "base-unix" {= "base"}
  "bigstringaf" {= "0.9.1"}
  "bwd" {= "2.3.0"}
  "cmdliner" {= "1.2.0"}
  "cppo" {= "1.6.9"}
  "csexp" {= "1.5.2"}
  "cstruct" {= "6.2.0"}
  "domain-local-await" {= "1.0.0"}
  "domain_shims" {= "0.1.0"}
  "domainslib" {= "0.5.1"}
  "dune" {= "3.11.1"}
  "dune-build-info" {= "3.11.1"}
  "dune-configurator" {= "3.11.1"}
  "eio" {= "0.12"}
  "eio_main" {= "0.12"}
  "eio_posix" {= "0.12"}
  "fmt" {= "0.9.0"}
  "hmap" {= "0.8.1"}
  "iomux" {= "0.3"}
  "lwt-dllist" {= "1.0.1"}
  "menhir" {= "20230608"}
  "menhirLib" {= "20230608"}
  "menhirSdk" {= "20230608"}
  "mtime" {= "2.0.0"}
  "ocaml" {= "5.1.0"}
  "ocaml-base-compiler" {= "5.1.0"}
  "ocaml-compiler-libs" {= "v0.12.4"}
  "ocaml-config" {= "3"}
  "ocaml-options-vanilla" {= "1"}
  "ocamlbuild" {= "0.14.2"}
  "ocamlfind" {= "1.9.6"}
  "ocamlgraph" {= "2.1.0"}
  "optint" {= "0.3.0"}
  "ppx_derivers" {= "1.2.1"}
  "ppx_deriving" {= "5.2.1"}
  "ppxlib" {= "0.31.0"}
  "psq" {= "0.2.1"}
  "ptime" {= "1.1.0"}
  "result" {= "1.5"}
  "saturn" {= "0.4.0"}
  "saturn_lockfree" {= "0.4.0"}
  "seq" {= "base"}
  "sexplib0" {= "v0.16.0"}
  "stdlib-shims" {= "0.3.0"}
  "thread-table" {= "1.0.0"}
  "topkg" {= "1.0.7"}
  "xmlm" {= "1.4.0"}
  "yuujinchou" {= "5.1.0"}
]
build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
dev-repo: "git+https://git.sr.ht/~jonsterling/ocaml-forester"